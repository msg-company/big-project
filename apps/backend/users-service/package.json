{
  "name": "users-service",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nest start --watch",
    "build": "NODE_ENV=production nest build",
    "start": "NODE_ENV=production nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --check \"{src,test}/**/*.ts\"",
    "format:fix": "prettier --write \"{src,test}/**/*.ts\"",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "migration:generate": "npx ts-node -P ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d ./src/app/configs/typeorm.config.ts ./src/app/migrations/migration",
    "migration:create": "npx ts-node -P ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create ./src/app/migrations/migration",
    "migration:run": "npx ts-node -P ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./src/app/configs/typeorm.config.ts",
    "migration:revert": "npx ts-node -P ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d ./src/app/configs/typeorm.config.ts",
    "db:migration:run": "npm run migration:run",
    "db:migration:generate": "npm run migration:generate",
    "db:migration:create": "npm run migration:create",
    "db:migration:revert": "npm run migration:revert"
  },
  "dependencies": {
    "@apollo/gateway": "^2.9.3",
    "@apollo/server": "^4.11.2",
    "@apollo/subgraph": "^2.9.3",
    "@nestjs/apollo": "^12.2.2",
    "@nestjs/common": "^10.4.15",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.15",
    "@nestjs/graphql": "^12.2.2",
    "@nestjs/platform-express": "^10.4.15",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/axios": "^3.1.3",
    "consul": "^2.0.1",
    "graphql": "^16.10.0",
    "graphql-tools": "^9.0.9",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "ts-morph": "^24.0.0",
    "@nestjs/microservices": "^10.4.15",
    "class-validator": "^0.14.1",
    "class-transformer": "^0.5.1",
    "typeorm": "^0.3.20",
    "@nestjs/typeorm": "^10.0.2",
    "kafkajs": "^2.2.4",
    "argon2": "^0.41.1",
    "@repo/env-config": "workspace:*",
    "@repo/service-discovery": "workspace:*",
    "pg": "^8.13.1",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.15",
    "@repo/eslint-config": "workspace:*",
    "@repo/prettier-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.4",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.17.0",
    "jest": "^29.7.0",
    "prettier": "link:@repo/prettier-config",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2",
    "@types/consul": "^0.40.3"
  }
}
